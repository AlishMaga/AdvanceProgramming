<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="366.0" prefWidth="590.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.finalproject.LoginController">
    <children>
        <Text text="Login Page">
            <font>
                <Font name="Arial" size="20.0" />
            </font>
        </Text>
        <TextField fx:id="usernameTextField" promptText="Username" />
        <PasswordField fx:id="passwordField" promptText="Password" />
        <Button fx:id="loginButton" onAction="#handleLoginButtonClick" text="Log In" />
      <Button fx:id="SignUp" onAction="#handleSignUpButton" mnemonicParsing="false" text="Sign Up" />
    </children>
</VBox>
